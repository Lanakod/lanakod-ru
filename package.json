{
  "name": "blog-backend",
  "version": "0.0.1",
  "description": "Lanakod.ru backend",
  "author": "Maxim Malakhov <lanakod.official@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "yarn typeorm:cli migration:generate --",
    "migration:create": "yarn mikro-orm migration:create",
    "migration:up": "yarn mikro-orm migration:up",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.6.9",
    "@mikro-orm/core": "^5.6.9",
    "@mikro-orm/migrations": "^5.6.9",
    "@mikro-orm/nestjs": "^5.1.7",
    "@mikro-orm/postgresql": "^5.6.9",
    "@mikro-orm/reflection": "^5.6.9",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "^9.2.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express-session": "^1.17.2",
    "ip": "^1.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.4",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/ip": "^1.1.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.3.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
